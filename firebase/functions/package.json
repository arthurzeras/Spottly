  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "private": true
